{
  "lib/*.ex": {
    "type": "lib",
    "alternate": "test/{}_test.exs",
    "template": [
      "defmodule {camelcase|capitalize|dot} do",
      "  @moduledoc \"\"\"",
      "  \"\"\"",
      "end"
    ]
  },
  "test/*_test.exs": {
    "type": "test",
    "alternate": "lib/{}.ex",
    "template": [
      "defmodule {camelcase|capitalize|dot}Test do",
      "  use ExUnit.Case",
      "end"
    ]
  },
  "mix.exs": {
    "type": "mix",
    "alternate": "mix.lock"
  },
  "mix.lock": {
    "type": "lock",
    "alternate": "mix.lock"
  }
}
